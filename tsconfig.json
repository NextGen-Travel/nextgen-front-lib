{
  "compilerOptions": {
    "target": "ES2022",
    "downlevelIteration": true,
    "module": "ES2022",
    "moduleResolution": "Node",
    "strict": true,
    "declaration": true,
    "outDir": "dist",
    "noEmit": true,
    "baseUrl": ".",
    "rootDir": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "ES2015"
    ]
  },
  "vueCompilerOptions": {
    "target": 2.7,
    "experimentalDisableTemplateSupport": true
  }
}
