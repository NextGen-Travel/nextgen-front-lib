{
  "compilerOptions": {
    "target": "ES2022",
    "downlevelIteration": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "declaration": true,
    "outDir": "types-vue",
    "noEmit": false,
    "baseUrl": ".",
    "rootDir": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "@amap/amap-jsapi-types",
      "@types/google.maps",
      "@types/apple-signin-api",
      "@types/facebook-js-sdk",
      "@types/google.accounts",
      "@types/node",
      "vuetify"
    ],
    "lib": [
      "esnext",
      "WebWorker",
      "dom"
    ]
  },
  "include": [
    "core/**/*.ts",
    "core/**/*.d.ts",
    "core/**/*.tsx",
    "core/**/*.vue",
    "./shims.d.ts"
  ]
}